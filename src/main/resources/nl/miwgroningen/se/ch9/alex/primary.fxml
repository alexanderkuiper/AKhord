<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="317.0" prefWidth="449.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.miwgroningen.se.ch9.alex.controller.PrimaryController">
   <children>
      <Label fx:id="toonLabel" text="Selecteer een toon" />
      <ChoiceBox fx:id="toonSelectorBox" prefHeight="25.0" prefWidth="38.0" />
      <Label fx:id="toonsoortLabel" text="Selecteer een toonsoort" />
      <ChoiceBox fx:id="toonsoortSelectorBox" prefHeight="25.0" prefWidth="107.0" />
      <Label fx:id="keuzeLabel" text="Kies een akkoord" />
      <Button fx:id="primaryButton" onAction="#submitUserChoice" text="OK" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
